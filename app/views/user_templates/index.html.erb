<div class="card col-md-8 offset-md-2">
  <div class="card-body">
    <h3 class="card-title">All Users</h3>
  </div>
  
  <ul class="list-group list-group-flush">
    <% @users.each do |user|%>  
        <li class="list-group-item"><a href="/users/<%=user.id%>"><%=user.username%></a></li>
    <%end%>    
  </ul>
</div>